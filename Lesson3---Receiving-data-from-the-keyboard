Targil 1:
import java.util.Scanner;
public class Ex1 
{
	public static void main(String[] args) 
	{
		Scanner scan = new Scanner(System.in);
		System.out.println("enter a number");
		int num = scan.nextInt();
		if (num<0)
			System.out.println("Negetive");
		else if (num>0)
			System.out.println("Positive");
		else
			System.out.println("None of them");			
	}
}

Targil 2:
import java.util.Scanner;
public class Ex2 
{
	public static void main(String[] args) 
	{
		Scanner scan = new Scanner(System.in);
		System.out.println("enter a number");
		int num1 = scan.nextInt();
		System.out.println("enter a number");
		int num2 = scan.nextInt();
		if (num1>num2)
			System.out.println(num1);
		else if (num2>num1)
			System.out.println(num2);
		else
			System.out.println("equal numbers");
	}
}

Targil 3:
import java.util.Scanner;
public class Ex3 
{
	public static void main(String[] args) 
	{
		Scanner scan = new Scanner(System.in);
		System.out.println("enter a letter");
		String letter1 = scan.next();
		System.out.println("enter a letter");
		String letter2 = scan.next();
		System.out.println("enter a distance");
		int distance1 = scan.nextInt();
		System.out.println("enter a distance");
		int distance2 = scan.nextInt();
		System.out.println("enter a speed");
		int speed1 = scan.nextInt();
		System.out.println("enter a speed");
		int speed2 = scan.nextInt();
		int t1 = distance1 / speed1;
		int t2 = distance2 / speed2;
		if(t1>t2)
			System.out.println(letter1);
		else
			System.out.println(letter2);
	}
}

Targil 4:
import java.util.Scanner;
public class Ex4 
{
	public static void main(String[] args) 
	{
		Scanner scan = new Scanner(System.in);
		System.out.println("enter a number");
		double num1 = scan.nextDouble();
		System.out.println("enter a number");
		double num2 = scan.nextDouble();
		System.out.println("enter a number");
		double num3 = scan.nextDouble();
		double avg = (num1+num2+num3)/3;
		System.out.println(avg);
	}
}

Targil 5:
import java.util.Scanner;
public class Ex5 
{
	public static void main(String[] args) 
	{
		Scanner scan = new Scanner(System.in);
		System.out.println("enter a number");
		int num1 = scan.nextInt();
		System.out.println("enter a number");
		int num2 = scan.nextInt();
		System.out.println("enter a number");
		int num3 = scan.nextInt();
		int minimum = Math.min(num1, num2);
		int minimum2 = Math.min(minimum, num3);
		System.out.println(minimum2);
	}
}

Targil 6:
import java.util.Scanner;
public class Ex6 
{
	public static void main(String[] args) 
	{
		Scanner scan = new Scanner(System.in);
		System.out.println("enter a number");
		int x1 = scan.nextInt();
		System.out.println("enter a number");
		int x2 = scan.nextInt();
		System.out.println("enter a number");
		int y1 = scan.nextInt();
		System.out.println("enter a number");
		int y2 = scan.nextInt();
		int x = x1-x2;
		double numx = Math.pow(x, 2);
		int y = y1-y2;
		double numy = Math.pow(y, 2);
		double number = numx + numy;
		double z = Math.sqrt(number);
		System.out.println(z);
	}
}

Targil 7:
import java.util.Scanner;
public class Ex7 
{
	public static void main(String[] args) 
	{
		Scanner scan = new Scanner(System.in);
		System.out.println("enter a number");
		int a1 = scan.nextInt();
		System.out.println("enter a number");
		int d = scan.nextInt();
		int n5 = a1 + (5-1) * d;
		int n10 = a1 + (10-1) *d;
		System.out.println(n5 + "and" + n10);
	}
}

Targil 8:
import java.util.Scanner;
public class Ex8 
{
	public static void main(String[] args) 
	{
		int unit;
		int complete,reminder;
		System.out.println("enter your number");
		Scanner scan = new Scanner(System.in);
		unit = scan.nextInt();
		if(unit>0 && unit<0)
			System.out.println(1.2 * unit);
		else
			if(unit ==10)
				System.out.print("10");
			else
				if(unit>10 && unit<54)
				{
					complete = unit /10;
					reminder = unit % 10;
					System.out.println((10 * complete) + (1.2 *reminder));
				}
				else
					if(unit == 54)
						System.out.println("50");
					else
						if(unit>54 && unit<108)
						{
							complete = (unit-54)/10;
							reminder = (unit-54)%10;
							System.out.println(50-(complete*10) + (reminder *1.2));
						}
						else
							if(unit==108)
								System.out.println("100");
							else
								System.out.print("Error");
		
	}
}

Targil 9:
import java.util.Scanner;
public class Ex9 
{
	public static void main(String[] args) 
	{
		Scanner scan = new Scanner(System.in);
		System.out.println("enter your full name");
		String name = scan.nextLine();
		System.out.println("enter your school's name");
		String school = scan.nextLine();
		System.out.println("enter the number of your subjects");
		int subjects = scan.nextInt();
		System.out.println("My name is" + name + "My schhol's name is" + school + "The number of subjects I study is" + subjects);
	}
}

Targil 10:
import java.util.Scanner;
public class Ex10 
{
	public static void main(String[] args) 
	{
		Scanner scan = new Scanner(System.in);
		System.out.println("enter a number");
		int num = scan.nextInt();
		int units = num %10;
		int tens = (num/10) % 10;
		int hundreds = num/100;
		System.out.println((units + tens + hundreds));
	}
}
